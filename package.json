{
  "name": "markdown-draft-js-gh-pages",
  "description": "Convert draftjs blocks to markdown using the marked library, and vice versa.",
  "author": "Rose Robertson",
  "version": "0.4.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rosey/markdown-draft-js.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "browserify src -o lib/index.js",
    "prepublish": "npm run build",
    "lint": "eslint ./src"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "eslint": "3.8.0",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "create-react-class": "^15.6.2",
    "draft-js": "^0.10.3",
    "markdown-draft-js": "^1.3.1",
    "react": "^16.0.0",
    "react-create-class": "^1.0.0",
    "react-dom": "^16.0.0",
    "remarkable": "1.7.1"
  }
}
